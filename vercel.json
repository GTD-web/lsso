{
    "version": 2,
    "builds": [
        {
            "src": "api/index.ts",
            "use": "@vercel/node",
            "config": {
                "includeFiles": ["src/**", "libs/**", "views/**", "public/**"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/index.ts",
            "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"]
        }
    ],
    "crons": [
        {
            "path": "/api/organization/cron/sync",
            "schedule": "0 0 * * *"
        }
    ]
}
